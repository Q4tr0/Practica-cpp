#include <iostream>
#include <math.h>
using namespace std;
int main(){
	float celsius=5;
	cout << "Tabla de conversion de grados celsius a farenheit";
	while(celsius<=50){
		
		//celsius=celsius/5;
		//celsius=celsius+32;
		cout<<"*"<<celsius<<"Convertido es"<<celsius*9/5+32<<endl;
		celsius+=5;
		
	}
system ("pause");
return 0;
}
